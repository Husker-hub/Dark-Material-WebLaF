<skin xmlns="http://weblookandfeel.com/XmlSkin">

    <!-- com.alee.laf.separator.WebSeparator -->

    <!-- Separator -->
    <style type="separator">
        <painter>
            <decorations>
                <decoration>
                    <SeparatorStripes>
                        <Stripe>
                            <color fraction="0.0">68,72,74,5</color>
                            <color fraction="0.5">68,72,74</color>
                            <color fraction="1.0">68,72,74,5</color>
                        </Stripe>
                        <Stripe>
                            <color fraction="0.0">40,40,40,5</color>
                            <color fraction="0.5">40,40,40</color>
                            <color fraction="1.0">40,40,40,5</color>
                        </Stripe>
                        <Stripe>
                            <color fraction="0.0">68,72,74,5</color>
                            <color fraction="0.5">68,72,74</color>
                            <color fraction="1.0">68,72,74,5</color>
                        </Stripe>
                    </SeparatorStripes>
                </decoration>
            </decorations>
        </painter>
    </style>

    <!-- Horizontal dark separator -->
    <style type="separator" id="horizontal">
        <component>
            <orientation>0</orientation>
        </component>
        <painter>
            <decorations>
                <decoration>
                    <SeparatorStripes>
                        <Stripe>
                            <color>100,100,100</color>
                        </Stripe>
                    </SeparatorStripes>
                </decoration>
            </decorations>
        </painter>
    </style>

    <!-- Vertical dark separator -->
    <style type="separator" id="vertical">
        <component>
            <orientation>1</orientation>
        </component>
        <painter>
            <decorations>
                <decoration>
                    <SeparatorStripes>
                        <Stripe>
                            <color>100,100,100</color>
                        </Stripe>
                    </SeparatorStripes>
                </decoration>
            </decorations>
        </painter>
    </style>

</skin>